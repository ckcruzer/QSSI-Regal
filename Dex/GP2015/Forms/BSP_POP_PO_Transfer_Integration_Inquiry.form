set PLookup:'PO Number' to '(L) PO Number To';
set PLookup:'Sort By' to 1;

set PLookup:'Called By' to 1;

set PLookup:'Include' to 0;
set PLookup:'Lock Include' to false;

call OpenWindowRet of form POP_Document_Lookup, PLookup, '(L) PO Number To';"
			UserProperties	""
		}
