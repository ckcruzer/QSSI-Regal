	~EventScripts
	{
		EventScript	"BSP_POP_PO_Transfer_Integration_Inquiry All Or Range2_CHG"		
		{
			Documentation	""
			Script	"clear '(L) PO Number From';
clear '(L) PO Number To';"
			UserProperties	""
		}
		EventScript	"BSP_POP_PO_Transfer_Integration_Inquiry Display Existing Record_CHG"		
		{
			Documentation	""
			Script	"release table BSP_Powerhouse_PO_Transfer;
range clear table BSP_Powerhouse_PO_Transfer;
clear table BSP_Powerhouse_PO_Transfer;

if 'All Or Range2' = 0 then 
	get first table BSP_Powerhouse_PO_Transfer;	
else
	if empty('(L) PO Number From') or empty('(L) PO Number To') then
		warning \"Please enter a value for the PO Number range\";
		abort script;
	else
		set 'PO Number' of table BSP_Powerhouse_PO_Transfer to '(L) PO Number From';
		range start table BSP_Powerhouse_PO_Transfer;
		set 'PO Number' of table BSP_Powerhouse_PO_Transfer to '(L) PO Number To';
		range end table BSP_Powerhouse_PO_Transfer;
		
		get first table BSP_Powerhouse_PO_Transfer;
	end if;
end if;

fill window BSP_POP_PO_Transfer_Integration_Inquiry_Scrolling;"
			UserProperties	""
		}
		EventScript	"BSP_POP_PO_Transfer_Integration_Inquiry Lookup Button 1_CHG"		
		{
			Documentation	""
			Script	"local POP_POLookup			PLookup;

clear PLookup;
set PLookup:'PO Number' to '(L) PO Number From';
set PLookup:'Sort By' to 1;

set PLookup:'Called By' to 1;

set PLookup:'Include' to 0;
set PLookup:'Lock Include' to false;

call OpenWindowRet of form POP_Document_Lookup, PLookup, '(L) PO Number From';"
			UserProperties	""
		}
		EventScript	"BSP_POP_PO_Transfer_Integration_Inquiry Lookup Button 2_CHG"		
