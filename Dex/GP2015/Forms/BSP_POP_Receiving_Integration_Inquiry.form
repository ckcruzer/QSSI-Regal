	~EventScripts
	{
		EventScript	"BSP_POP_Receiving_Integration_Maintenance BSP_ContainerID_CHG"		
		{
			Documentation	""
			Script	"run script 'Display Existing Record';"
			UserProperties	""
		}
		EventScript	"BSP_POP_Receiving_Integration_Maintenance Delete Button_CHG"		
		{
			Documentation	""
			Script	"local integer	nStatus;
local string 	l_ErrorMessage;

if empty(BSP_ContainerID) then
	warning \"Please enter a Container ID\";
	abort script;
end if;

if ask(\"Deleting this record will also delete the record in Powerhouse. Are you sure you want to delete this container record? \", \"Yes\", \"No\", \"Cancel\") = ASKBUTTON1 then
	call DeleteRecord of form BSP_POP_Receiving_Integration_Maintenance, BSP_ContainerID, nStatus, l_ErrorMessage;
	
	if nStatus = OKAY then
		warning \"Record has been successfully deleted.\";
		restart form;
	else
		warning \"There was an error processing your request. \" + l_ErrorMessage;
		abort script;
	end if;
end if;"
			UserProperties	""
		}
		EventScript	"BSP_POP_Receiving_Integration_Maintenance Display Existing Record_CHG"		
		{
			Documentation	""
			Script	"{fill window and then disable the \"include\"}
if not empty(BSP_ContainerID) then
	release table BSP_Powerhouse_Receiving_HDR;
	range clear table BSP_Powerhouse_Receiving_HDR;
	clear table BSP_Powerhouse_Receiving_HDR;
	
	set BSP_ContainerID of table BSP_Powerhouse_Receiving_HDR to BSP_ContainerID;
	get table BSP_Powerhouse_Receiving_HDR;
	if err() = OKAY then
		if 'Status Code' of table BSP_Powerhouse_Receiving_HDR = \"X\" then
			set 'Display Existing Record' to true;
		end if;
		
		copy from table BSP_Powerhouse_Receiving_HDR to window BSP_POP_Receiving_Integration_Maintenance;
		
		release table BSP_Powerhouse_Receiving_LINE;
		range clear table BSP_Powerhouse_Receiving_LINE;
		clear table BSP_Powerhouse_Receiving_LINE;
		
		set BSP_ContainerID of table BSP_Powerhouse_Receiving_LINE to BSP_ContainerID;
		clear 'POP Receipt Number' of table BSP_Powerhouse_Receiving_LINE;
		range start table BSP_Powerhouse_Receiving_LINE;
		fill 'POP Receipt Number' of table BSP_Powerhouse_Receiving_LINE;
		range end table BSP_Powerhouse_Receiving_LINE; 
		
		fill window BSP_POP_Receiving_Integration_Maintenance_Scrolling;
	else
		set 'Display Existing Record' to false;
		warning \"Container ID record does not exist\";
	end if;
	
end if;
"
			UserProperties	""
		}
		EventScript	"BSP_POP_Receiving_Integration_Maintenance Lookup Button 3_CHG"		
		{
			Documentation	""
			Script	"open form BSP_POP_Receiving_Integration_Lookup return to BSP_ContainerID;"
			UserProperties	""
		}
		EventScript	"BSP_POP_Receiving_Integration_Maintenance OK Button_CHG"		
		{
			Documentation	""
			Script	"close form BSP_POP_Receiving_Integration_Maintenance;"
			UserProperties	""
		}
		EventScript	"BSP_POP_Receiving_Integration_Maintenance Zoom Buttons[1]_CHG"		
		{
			Documentation	""
			Script	"local	integer	nStatus,
				nOrigin,
				nDocType;


set nOrigin to HISTRCPT;

if not empty('POP Receipt Number' of window BSP_POP_Receiving_Integration_Maintenance_Scrolling)then
	nDocType = GetDocTypeForReceipt('POP Receipt Number' of window BSP_POP_Receiving_Integration_Maintenance_Scrolling) of form POP_ReceiptHist;
	
	case nDocType
		in [POP_DOCTYPE_INVOICE]
			nStatus = OpenWindow('POP Receipt Number' of window BSP_POP_Receiving_Integration_Maintenance_Scrolling,
								POP_BYNOTSET,
								nOrigin,
								CURRENCYVIEW_FUNCTIONAL) of form POP_Inquiry_Invoice_Entry;
				
		in [POP_DOCTYPE_SHIPMENT, POP_DOCTYPE_SHIPMENT_INVOICE, POP_DOCTYPE_IN_TRANSIT]
			nStatus = OpenWindow('POP Receipt Number' of window BSP_POP_Receiving_Integration_Maintenance_Scrolling,
								POP_BYNOTSET,
								nOrigin,
								CURRENCYVIEW_FUNCTIONAL) of form POP_Inquiry_Receivings_Entry;
				
		in [POP_DOCTYPE_RETURN, POP_DOCTYPE_RETURNCREDIT, POP_DOCTYPE_INVENTORYRETURN, POP_DOCTYPE_INVENTORYRETURNCREDIT]
			nStatus = OpenWindow('POP Receipt Number' of window BSP_POP_Receiving_Integration_Maintenance_Scrolling,
								nOrigin,
								POP_BYNOTSET,
								CURRENCYVIEW_FUNCTIONAL) of form POR_Inquiry_Returns_Entry;
		in [MISSING]
			call Warning_Dialog, 1391;
	end case;
else
	call Warning_Dialog, 6007;
end if;"
			UserProperties	""
		}
		EventScript	"BSP_POP_Receiving_Integration_Maintenance_WIN_PRE"		
		{
			Documentation	""
			Script	"expand window BSP_POP_Receiving_Integration_Maintenance_Scrolling, true;"
			UserProperties	""
