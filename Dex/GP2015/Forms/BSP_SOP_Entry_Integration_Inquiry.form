	~EventScripts
	{
		EventScript	"BSP_SOP_Entry_Integration_Inquiry All Or Range2_CHG"		
		{
			Documentation	""
			Script	"clear '(L) SOP Number From';
clear '(L) SOP Number To';"
			UserProperties	""
		}
		EventScript	"BSP_SOP_Entry_Integration_Inquiry Display Existing Record_CHG"		
		{
			Documentation	""
			Script	"release table BSP_Powerhouse_SO_Log;
range clear table BSP_Powerhouse_SO_Log;
clear table BSP_Powerhouse_SO_Log;

if 'All Or Range2' = 0 then 
	get first table BSP_Powerhouse_SO_Log;	
else
	if empty('(L) SOP Number From') or empty('(L) SOP Number To') then
		warning \"Please enter a value for the SOP Number range\";
		abort script;
	else
		set 'SOP Number' of table BSP_Powerhouse_SO_Log to '(L) SOP Number From';
		set 'SOP Type' of table BSP_Powerhouse_SO_Log to SOP_ORDER;
		set VersionNumber of table BSP_Powerhouse_SO_Log to \"0\";
		range start table BSP_Powerhouse_SO_Log;
		set 'SOP Number' of table BSP_Powerhouse_SO_Log to '(L) SOP Number To';
		set 'SOP Type' of table BSP_Powerhouse_SO_Log to SOP_ORDER;
		set VersionNumber of table BSP_Powerhouse_SO_Log to \"0\";
		range end table BSP_Powerhouse_SO_Log;
		
		get first table BSP_Powerhouse_SO_Log;
	end if;
end if;

fill window BSP_SOP_Entry_Integration_Inquiry_Scrolling;"
			UserProperties	""
		}
		EventScript	"BSP_SOP_Entry_Integration_Inquiry Lookup Button 1_CHG"		
		{
			Documentation	""
			Script	"local	date			l_Empty_Date;

open form SOP_Document_Lookup return to '(L) SOP Number From';
call OPEN of form SOP_Document_Lookup,
		1, {sort by}
		SHOW_ALL of form SOP_Document_Lookup,
		0,	
		1,	{locked}
		SOP_ORDER, {type}
		'(L) SOP Number From',
		\"\", {customer #}
		\"\", {batch #}
		\"\", {trx source}
		l_Empty_Date, {document date}
		0,	{master number}
		false,  {opening from SOP_Entry}
		SOP_ORDER; {restrict to SOP Type}"
			UserProperties	""
		}
