Function	"get_bind_type"
{
	Core	"System"
	Documentation	""
	Script	"function returns string sType;

in long iType;

{
175 = char
52 = currency
61 = date/time
108 = int/small
35 = text
}

case iType
	in [175] sType = \"GPS_CHAR\";
	in [52, 106]	sType = \"GPS_MONEY\";
	in [61] sType = \"GPS_DATE\";
	in [108, 48, 56] sType = \"GPS_INT\";
end case;

"
	UserProperties	""
}
