		EventScript	"BSP_SOP_Entry_Integration l_Send to Powerhouse_CHG"		
		{
			Documentation	""
			Script	"local integer				nStatus;
local 'Error Message Text' 	l_ErrorMessage;
local 'Batch Number'		l_ProcessedBatch;
local 'Batch Number'		l_RejectedBatch;

{Retrieve Batches}
release table BSP_Integration_SETP;
range clear table BSP_Integration_SETP;
clear table BSP_Integration_SETP;

set 'Company ID' of table BSP_Integration_SETP to 'Company ID' of globals;
get table BSP_Integration_SETP;
if err() = OKAY then
	set l_ProcessedBatch to BSP_SOPProcessedBatch of table BSP_Integration_SETP;
	set l_RejectedBatch to BSP_SOPRejectedBatch of table BSP_Integration_SETP;
else
	abort script;
end if;

if ask(\"Are you sure you want to send this record to Powerhouse?\", \"Yes\", \"No\", \"\") = ASKBUTTON1 then
	call SendToPowerhouse of form BSP_SOP_Entry_Integration, 'SOP Type', 'SOP Number', nStatus, l_ErrorMessage;
	
	if nStatus = OKAY then
		warning \"Record has been successfully sent to Powerhouse.\";
		set '(L) NewBatchNumber' to l_ProcessedBatch;
	else
		warning \"There was an error processing your request. \" + l_ErrorMessage;
		set '(L) NewBatchNumber' to l_RejectedBatch;
		abort script;
	end if;
	
	run script 'Display Existing Record';
end if;"
			UserProperties	""
		}
		EventScript	"BSP_SOP_Entry_Integration l_ServiceSendToPowerhouse_CHG"		
		{
			Documentation	""
			Script	"local 'Batch Number'		l_SOPBatch;
local 'SOP Number'			l_SOPNumber;
local 'SOP Type'			l_SOPType;
local integer				nStatus = OKAY;
local 'Error Message Text' 	l_ErrorMessage = \"\";

local 'Batch Number'		l_ProcessedBatch;
local 'Batch Number'		l_RejectedBatch;


local 	'SOP Number'			sSOPNumber;
local	integer					nSOPType;	

release table BSP_Integration_SETP;
range clear table BSP_Integration_SETP;
clear table BSP_Integration_SETP;

set 'Company ID' of table BSP_Integration_SETP to 'Company ID' of globals;
get table BSP_Integration_SETP;
if err() = OKAY then
	set l_SOPBatch to BSP_SOPBatchID of table BSP_Integration_SETP;
	set l_ProcessedBatch to BSP_SOPProcessedBatch of table BSP_Integration_SETP;
	set l_RejectedBatch to BSP_SOPRejectedBatch of table BSP_Integration_SETP;
else
	abort script;
end if;

{Check if SOP record exists just in case the SOP Entry form was closed}
release table SOP_HDR_WORK;
range clear table SOP_HDR_WORK;
clear table SOP_HDR_WORK;

set 'Batch Source' of table SOP_HDR_WORK to BATCH_SOURCE_SOP_ENTRY;
set 'Batch Number' of table SOP_HDR_WORK to l_SOPBatch;
range start table SOP_HDR_WORK by SOP_HDR_WORK_Key4;
set 'Batch Source' of table SOP_HDR_WORK to BATCH_SOURCE_SOP_ENTRY;
set 'Batch Number' of table SOP_HDR_WORK to l_SOPBatch;
fill 'SOP Type' of table SOP_HDR_WORK;
fill 'SOP Number' of table SOP_HDR_WORK;
range end table SOP_HDR_WORK by SOP_HDR_WORK_Key4;

change first table SOP_HDR_WORK by SOP_HDR_WORK_Key4;
while err() = OKAY do
	set l_SOPNumber to 'SOP Number' of table SOP_HDR_WORK;
	set l_SOPType to 'SOP Type' of table SOP_HDR_WORK;
	
	{Check first if this record has already been successfully sent before. If yes, skip}
	release table BSP_Powerhouse_SO_LNE_Transfer;
	range clear table BSP_Powerhouse_SO_LNE_Transfer;
	clear table BSP_Powerhouse_SO_LNE_Transfer;
	
	set 'SOP Type' of table BSP_Powerhouse_SO_LNE_Transfer to l_SOPType;
	set 'SOP Number' of table BSP_Powerhouse_SO_LNE_Transfer to l_SOPNumber;
	range start table BSP_Powerhouse_SO_LNE_Transfer;
	set 'SOP Type' of table BSP_Powerhouse_SO_LNE_Transfer to l_SOPType;
	set 'SOP Number' of table BSP_Powerhouse_SO_LNE_Transfer to l_SOPNumber;
	fill 'Component Sequence' of table BSP_Powerhouse_SO_LNE_Transfer;
	fill 'Line Item Sequence' of table BSP_Powerhouse_SO_LNE_Transfer;
	range end table BSP_Powerhouse_SO_LNE_Transfer;
	
	get first table BSP_Powerhouse_SO_LNE_Transfer;
	if err() = OKAY then
		release table SOP_HDR_WORK;
		change next table SOP_HDR_WORK by SOP_HDR_WORK_Key4; 
		continue while;
	end if;
	
	if ('Batch Number' of table SOP_HDR_WORK = l_SOPBatch) then {double check the batch}
		call SendToPowerhouse of form BSP_SOP_Entry_Integration, l_SOPType, l_SOPNumber, nStatus, l_ErrorMessage;
		if nStatus = 1 then
			set 'Batch Number' of table SOP_HDR_WORK to l_RejectedBatch;
			save table SOP_HDR_WORK;
			call UpdateSalesOrderBatch of form BSP_SOP_Entry_Integration, l_RejectedBatch, l_SOPBatch, 'Document Amount' of table SOP_HDR_WORK;
		else
			{set 'Batch Number' of table SOP_HDR_WORK to l_ProcessedBatch;
			save table SOP_HDR_WORK;
			call UpdateSalesOrderBatch of form BSP_SOP_Entry_Integration, l_ProcessedBatch, l_SOPBatch, 'Document Amount' of table SOP_HDR_WORK;} 
			{Removed this for now since we do not need to update} 
			release table SOP_HDR_WORK;
		end if;
	else
		release table SOP_HDR_WORK;
	end if;
	
	change next table SOP_HDR_WORK by SOP_HDR_WORK_Key4; 
end while;




"
			UserProperties	""
		}
