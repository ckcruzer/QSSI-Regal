	~Procedures
	{
		Procedure	"SendToPowerhouse"		
		{
			Documentation	""
			Script	"in 'SOP Type' 				in_SOPType;
in 'SOP Number' 			in_SOPNumber;
out integer					out_Result;
out 'Error Message Text' 	out_ErrorMessageText;"
			UserProperties	""
		}
		Procedure	"UpdateSalesOrderBatch"		
		{
			Documentation	""
			Script	"in 'Batch Number' IN_NewBatchNumber;
in 'Batch Number' IN_OldBatchNumber;
in 'Document Amount' IN_DocumentAmount;

if IN_NewBatchNumber <> IN_OldBatchNumber then
	
	if not empty(IN_OldBatchNumber) then
		repeat
			set 'Batch Source' of table Batch_Headers to BATCH_SOURCE_SOP_ENTRY;
			set 'Batch Number' of table Batch_Headers to IN_OldBatchNumber;
			change table Batch_Headers;
			if err() = OKAY then
				set 'Number Of TRX' of table Batch_Headers to 
					'Number Of TRX' of table Batch_Headers - 1;
				set 'Batch Total' of table Batch_Headers to 
					'Batch Total' of table Batch_Headers - IN_DocumentAmount;
				save table Batch_Headers;
			end if;
		until err() <> RECORDCHANGED;
	end if;

	if not empty(IN_NewBatchNumber) then
		repeat
			set 'Batch Source' of table Batch_Headers to BATCH_SOURCE_SOP_ENTRY; 
			set 'Batch Number' of table Batch_Headers to IN_NewBatchNumber;
			change table Batch_Headers;
			if err() = OKAY then	
				increment 'Number Of TRX' of table Batch_Headers;
				increment 'Batch Total' of table Batch_Headers by IN_DocumentAmount;
				save table Batch_Headers;
			end if;
		until err() <> RECORDCHANGED;
	end if;

end if;"
			UserProperties	""
		}
	}
}
