		EventScript	"BSP_Powerhouse_Location_Mapping_Scrolling SCROLL_CHG"		
		{
			Documentation	""
			Script	"if empty('Location Code' of window BSP_Powerhouse_Location_Mapping_Scrolling) then
	warning \"Please enter a Location Code\";
	abort script;
end if;

if empty(BSP_PHWarehouseID of window BSP_Powerhouse_Location_Mapping_Scrolling) then
	warning \"Please enter a Powerhouse Warehouse ID\";
	abort script;
end if;

release table BSP_Powerhouse_Location_Mapping;
range clear table BSP_Powerhouse_Location_Mapping;
clear table BSP_Powerhouse_Location_Mapping;

set BSP_PHWarehouseID of table BSP_Powerhouse_Location_Mapping to BSP_PHWarehouseID of window BSP_Powerhouse_Location_Mapping_Scrolling;
set 'Location Code' of table BSP_Powerhouse_Location_Mapping to 'Location Code' of window BSP_Powerhouse_Location_Mapping_Scrolling;
change table BSP_Powerhouse_Location_Mapping;
save table BSP_Powerhouse_Location_Mapping;"
			UserProperties	""
		}
